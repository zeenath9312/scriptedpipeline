Jenkinsfile (Declarative Pipeline)
pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                echo "a"
            }
        }
        stage('Test') { 
            steps {
                echo "b"
            }
        }
        stage('Deploy') { 
            steps {
                echo "c"
            }
        }
    }
}
